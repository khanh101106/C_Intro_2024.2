#include <stdio.h>

int main() {
    float x;
    scanf("%f", &x);

    // Check if the score is valid
    if (x < 0.0 || x > 10.0) {
        printf("ERROR\n");
    } else {
        // Convert score to corresponding grade
        if (x < 4.0) {
            printf("F\n");
        } else if (x >= 4.0 && x < 5.0) {
            printf("D\n");
        } else if (x >= 5.0 && x < 5.5) {
            printf("D+\n");
        } else if (x >= 5.5 && x < 6.5) {
            printf("C\n");
        } else if (x >= 6.5 && x < 7.0) {
            printf("C+\n");
        } else if (x >= 7.0 && x < 8.0) {
            printf("B\n");
        } else if (x >= 8.0 && x < 8.5) {
            printf("B+\n");
        } else if (x >= 8.5 && x < 9.5) {
            printf("A\n");
        } else {
            printf("A+\n");
        }
    }

    return 0;
}
